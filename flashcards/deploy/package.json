{
  "name": "@simple-flashcards/deploy",
  "version": "1.0.0",
  "description": "Simple Flashcards Deployment Scripts",
  "main": "deploy.ts",
  "license": "MIT",
  "scripts": {
    "deploy": "DOTENV_CONFIG_PATH=../.env ts-node -r dotenv/config deploy.ts",
    "deploy:dev": "DOTENV_CONFIG_PATH=../.env ts-node -r dotenv/config deploy.ts --stage dev",
    "deploy:prod": "DOTENV_CONFIG_PATH=../.env ts-node -r dotenv/config deploy.ts --stage prod"
  },
  "dependencies": {
    "@aws-sdk/client-appsync": "^3.955.0",
    "@aws-sdk/client-cloudformation": "^3.300.0",
    "@aws-sdk/client-cloudfront": "^3.817.0",
    "@aws-sdk/client-dynamodb": "^3.817.0",
    "@aws-sdk/client-s3": "^3.300.0",
    "@aws-sdk/client-sts": "^3.954.0",
    "@types/archiver": "^7.0.0",
    "archiver": "^7.0.1",
    "chalk": "^4.1.2",
    "commander": "^9.4.0",
    "dotenv": "^16.5.0",
    "esbuild": "^0.20.0",
    "fs-extra": "^11.3.0",
    "glob": "^10.0.0",
    "inquirer": "^8.2.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/inquirer": "^8.2.4",
    "@types/node": "^18.11.9",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
